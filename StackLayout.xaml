<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FlexLayoutDemo_RERS.StackLayout"
             Title="StackLayout">
    <StackLayout Padding="9" Spacing="10">
        <BoxView HeightRequest="50" BackgroundColor="Blue"/>
        <BoxView HeightRequest="50" BackgroundColor="Yellow"/>
        <BoxView HeightRequest="40" BackgroundColor="CadetBlue"/>
        <BoxView HeightRequest="40" BackgroundColor="CadetBlue"/>
        <BoxView HeightRequest="40" BackgroundColor="CadetBlue"/>
        </StackLayout>
</ContentPage>